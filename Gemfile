gemspec

gem 'rake', '~> 12.0'

gem 'activesupport'
gem 'simplecov', require: false, group: :test

group :development, :test do
  gem 'rspec', '~> 3.0'  # RubyMine does not detect when RSpec is added under a group within the Gemfile
end